uses Bitmap

class VMGImage
  PROPERTIES
    bitmap : Bitmap
    name   : String

  METHODS
    method init( bitmap, name=null )

    method name->String
      local dims = "[$x$]"(bitmap.width,bitmap.height)
      if (@name) return "$ $"(@name,dims)
      return dims

    method to->String
      return name
endClass

